# Configuración de la aplicación FutDAO

# Red de Blockchain
REACT_APP_BLOCKCHAIN_NETWORK=substrate-testnet
REACT_APP_RPC_URL=wss://substrate-testnet.polkadot.io
REACT_APP_WS_URL=wss://substrate-testnet.polkadot.io

# Direcciones de Contratos
REACT_APP_GOVERNANCE_CONTRACT=0x1234567890abcdef1234567890abcdef12345678
REACT_APP_NFT_CONTRACT=0xabcdef1234567890abcdef1234567890abcdef12
REACT_APP_TOKEN_CONTRACT=0x9876543210fedcba9876543210fedcba98765432

# Configuración de la API
REACT_APP_API_URL=https://api.futdao.com
REACT_APP_API_VERSION=v1

# Configuración de la aplicación
REACT_APP_APP_NAME=FutDAO
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=Framework para Gestión de Clubes Deportivos Basado en DAO y Web3

# Configuración de desarrollo
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Configuración de producción
# REACT_APP_DEBUG=false
# REACT_APP_LOG_LEVEL=error

# Configuración de analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Configuración de notificaciones
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id

# Configuración de redes sociales
REACT_APP_TWITTER_URL=https://twitter.com/futdao
REACT_APP_DISCORD_URL=https://discord.gg/futdao
REACT_APP_TELEGRAM_URL=https://t.me/futdao

# Configuración de soporte
REACT_APP_SUPPORT_EMAIL=support@futdao.com
REACT_APP_DOCS_URL=https://docs.futdao.com

# Configuración de monedas
REACT_APP_DEFAULT_CURRENCY=EUR
REACT_APP_SUPPORTED_CURRENCIES=EUR,USD,FDT

# Configuración de idiomas
REACT_APP_DEFAULT_LANGUAGE=es
REACT_APP_SUPPORTED_LANGUAGES=es,en,fr,de,it,pt

# Configuración de temas
REACT_APP_DEFAULT_THEME=light
REACT_APP_SUPPORTED_THEMES=light,dark

# Configuración de paginación
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=100

# Configuración de cache
REACT_APP_CACHE_TTL=300000
REACT_APP_CACHE_MAX_SIZE=50

# Configuración de rate limiting
REACT_APP_RATE_LIMIT_REQUESTS=100
REACT_APP_RATE_LIMIT_WINDOW=60000

# Configuración de seguridad
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_LOCKOUT_DURATION=900000

# Configuración de backup
REACT_APP_BACKUP_INTERVAL=86400000
REACT_APP_BACKUP_RETENTION=7

# Configuración de monitoreo
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_LOGROCKET_APP_ID=your_logrocket_app_id

# Configuración de testing
REACT_APP_TEST_MODE=false
REACT_APP_MOCK_API=false
REACT_APP_TEST_USER_ID=test_user_123

# Configuración de features flags
REACT_APP_FEATURE_NFT_MARKETPLACE=true
REACT_APP_FEATURE_GOVERNANCE=true
REACT_APP_FEATURE_PLAYER_MANAGEMENT=true
REACT_APP_FEATURE_BUDGET_MANAGEMENT=true
REACT_APP_FEATURE_USER_PROFILES=true
REACT_APP_FEATURE_BLOCKCHAIN_INTEGRATION=true
REACT_APP_FEATURE_NOTIFICATIONS=true
REACT_APP_FEATURE_ANALYTICS=true
REACT_APP_FEATURE_SOCIAL_FEATURES=true
REACT_APP_FEATURE_MOBILE_APP=true
